{
  "name": "@aboutcircles/sdk-core",
  "version": "0.1.15",
  "description": "Circles Contracts wrapper",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./hubV2": {
      "types": "./dist/contracts/hubV2.d.ts",
      "default": "./dist/contracts/hubV2.js"
    },
    "./baseGroup": {
      "types": "./dist/contracts/baseGroup.d.ts",
      "default": "./dist/contracts/baseGroup.js"
    },
    "./baseGroupFactory": {
      "types": "./dist/contracts/baseGroupFactory.d.ts",
      "default": "./dist/contracts/baseGroupFactory.js"
    },
    "./nameRegistry": {
      "types": "./dist/contracts/nameRegistry.d.ts",
      "default": "./dist/contracts/nameRegistry.js"
    },
    "./liftERC20": {
      "types": "./dist/contracts/liftERC20.d.ts",
      "default": "./dist/contracts/liftERC20.js"
    },
    "./invitationFarm": {
      "types": "./dist/contracts/invitationFarm.d.ts",
      "default": "./dist/contracts/invitationFarm.js"
    },
    "./referralsModule": {
      "types": "./dist/contracts/referralsModule.d.ts",
      "default": "./dist/contracts/referralsModule.js"
    },
    "./inflationaryCircles": {
      "types": "./dist/contracts/inflationaryCircles.d.ts",
      "default": "./dist/contracts/inflationaryCircles.js"
    },
    "./demurrageCircles": {
      "types": "./dist/contracts/demurrageCircles.d.ts",
      "default": "./dist/contracts/demurrageCircles.js"
    },
    "./minimal": {
      "types": "./dist/contracts/minimal/index.d.ts",
      "default": "./dist/contracts/minimal/index.js"
    },
    "./minimal/hubV2": {
      "types": "./dist/contracts/minimal/hubV2.d.ts",
      "default": "./dist/contracts/minimal/hubV2.js"
    },
    "./minimal/liftERC20": {
      "types": "./dist/contracts/minimal/liftERC20.d.ts",
      "default": "./dist/contracts/minimal/liftERC20.js"
    },
    "./minimal/wrappedCircles": {
      "types": "./dist/contracts/minimal/wrappedCircles.d.ts",
      "default": "./dist/contracts/minimal/wrappedCircles.js"
    },
    "./minimal/referralsModule": {
      "types": "./dist/contracts/minimal/referralsModule.d.ts",
      "default": "./dist/contracts/minimal/referralsModule.js"
    },
    "./minimal/invitationFarm": {
      "types": "./dist/contracts/minimal/invitationFarm.d.ts",
      "default": "./dist/contracts/minimal/invitationFarm.js"
    },
    "./contracts/minimal": {
      "types": "./dist/contracts/minimal/index.d.ts",
      "default": "./dist/contracts/minimal/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aboutcircles/sdk"
  },
  "license": "MIT",
  "scripts": {
    "build": "bun build ./src/index.ts ./src/contracts/minimal/index.ts ./src/contracts/minimal/hubV2.ts ./src/contracts/minimal/liftERC20.ts ./src/contracts/minimal/wrappedCircles.ts ./src/contracts/minimal/referralsModule.ts ./src/contracts/minimal/invitationFarm.ts --outdir ./dist --format esm --minify --splitting && tsc --emitDeclarationOnly",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@aboutcircles/sdk-abis": "*",
    "@aboutcircles/sdk-types": "*",
    "@aboutcircles/sdk-utils": "*",
    "abitype": "^1.1.1"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@types/bun": "^1.2.23",
    "esbuild": "^0.25.10",
    "typescript": "^5.0.4"
  }
}
